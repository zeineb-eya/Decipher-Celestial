{% extends ('baseback/base.html.twig') %}


{% block title %}Planning
{% endblock %}

{% block body %}
	<h1>PLanning</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ planinng.id }}</td>
			</tr>

			<tr>
				<th>Nom_planning</th>
				<td>{{ planinng.nomPlanning }}</td>
			</tr>
			<tr>
				<th>DateDebut_planning</th>
				<td>{{ planinng.dateDebutPlanning ? planinng.dateDebutPlanning|date('Y-m-d') : '' }}</td>
			</tr>
			<tr>
				<th>DateFin_planning</th>
				<td>{{ planinng.dateFinPlanning ? planinng.dateFinPlanning|date('Y-m-d') : '' }}</td>
			</tr>
			<tr>
				<th>Destination_planning</th>
				<td>{{ planinng.destinationPlanning }}</td>
			</tr>
			
			<tr>
				<th>Description_planning</th>
				<td>{{ planinng.descriptionPlanning }}</td>
			</tr>
			<tr>
				<th>Periode_planning</th>
				<td>{{ planinng.periodePlanning }}</td>
			</tr>
			<tr>
				<th>Prix_planning</th>
				<td>{{ planinng.prixPlanning }}</td>
			</tr>
			<tr>
				<th>Img_planning</th>
				<td>{{ planinng.imgPlaninng }}</td>
			</tr>
		</tbody>
	</table>
	<a href="{{ path('planinng_index') }}"class="btn btn-primary">back to list</a>

	<a href="{{ path('planinng_edit', {'id': planinng.id}) }}"class="btn btn-primary">edit</a>

	{{ include('planinng/_delete_form.html.twig') }}
{% endblock %}
